<hibernate-mapping>
    <class catalog="sede" name="entities.Registro" table="registro">
        
        <id name="idRegistro" column="idRegistro" type="string" length="20" not-null="true">
            <generator class="assigned" />
        </id>
        <property name="dniSolicitante" type="string" not-null="true"/>
        <property name="nombreSolicitante" type="string" not-null="true"/>
        <property name="apellidosSolicitante" type="string" not-null="true"/>
        <property name="tramite" type="string" not-null="true"/>
        <property name="entidad" type="string" not-null="true"/>
        
    </class>
</hibernate-mapping>